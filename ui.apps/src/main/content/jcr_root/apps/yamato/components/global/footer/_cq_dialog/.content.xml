<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer Navigation"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <mainHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}3"
                                                text="Main"/>
                                            <logoHref
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldDescription="ロゴ画像のリンク先"
                                                fieldLabel="ロゴURL"
                                                name="./logoUrl"
                                                rootPath="/content/yamato"
                                                required="{Boolean}true"/>
                                            <logoImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="ロゴ画像パス"
                                                name="./logoImagePath"
                                                required="{Boolean}true"
                                                rootPath="/content/dam/yamato"/>
                                            <alt
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="ロゴ画像のalt属性"
                                                name="./logoAlt"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <footerNavi1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="フッターナビ1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <navigation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./navi1/items">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <naviRoot
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            defaultValue="/content/yamato/jp/ja"
                                                            fieldDescription="各ナビゲーションのルートパス"
                                                            fieldLabel="ナビルート"
                                                            name="./naviRoot"
                                                            rootPath="/content/yamato/"/>
                                                        <showChild
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                            name="./showChild"
                                                            text="子ページを表示する"
                                                            value="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </navigation>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </footerNavi1>
                    <footerNavi2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="フッターナビ2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <navigation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./navi2/items">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <naviRoot
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            defaultValue="/content/yamato/jp/ja"
                                                            fieldDescription="各ナビゲーションのルートパス"
                                                            fieldLabel="ナビルート"
                                                            name="./naviRoot"
                                                            rootPath="/content/yamato/"/>
                                                        <showChild
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                            name="./showChild"
                                                            text="子ページを表示する"
                                                            value="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </navigation>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </footerNavi2>
                    <footerNavi3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="フッターナビ3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <navigation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./navi3/items">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <naviRoot
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            defaultValue="/content/yamato/jp/ja"
                                                            fieldDescription="各ナビゲーションのルートパス"
                                                            fieldLabel="ナビルート"
                                                            name="./naviRoot"
                                                            rootPath="/content/yamato/"/>
                                                        <showChild
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                            name="./showChild"
                                                            text="子ページを表示する"
                                                            value="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </navigation>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </footerNavi3>
                    <footerNavi4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="フッターナビ4"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <navigation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./navi4/items">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <naviRoot
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            defaultValue="/content/yamato/jp/ja"
                                                            fieldDescription="各ナビゲーションのルートパス"
                                                            fieldLabel="ナビルート"
                                                            name="./naviRoot"
                                                            rootPath="/content/yamato/"/>
                                                        <showChild
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                            name="./showChild"
                                                            text="子ページを表示する"
                                                            value="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </navigation>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </footerNavi4>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
